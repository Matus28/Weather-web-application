{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/index.ts",
    "build": "rimraf dist && tsc --project tsconfig.build.json",
    "ts.check": "tsc --project tsconfig.build.json",
    "add-build": "git add dist",
    "test": "cross-env NODE_ENV=test jest --testTimeout=5000",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\""
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.5",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.14",
    "async-series": "^0.0.1",
    "concurrently": "^7.6.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "pre-commit": "^1.2.2",
    "rimraf": "^4.4.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "validator": "^13.9.0"
  }
}
